{
  "name": "openclaw-nest",
  "version": "0.1.0",
  "type": "module",
  "description": "OpenClaw multi-instance manager with Docker/Process engine support",
  "bin": {
    "openclaw-nest": "./bin/nest.js"
  },
  "scripts": {
    "start": "node bin/nest.js",
    "build": "node scripts/build.js",
    "package": "pkg dist/nest.cjs --targets node22-win-x64,node22-macos-x64,node22-macos-arm64,node22-linux-x64,node22-linux-arm64 --output dist/openclaw-nest --compress GZip"
  },
  "dependencies": {
    "dockerode": "^4.0.9",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@yao-pkg/pkg": "^6.5.0",
    "esbuild": "^0.25.0"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "license": "MIT"
}
